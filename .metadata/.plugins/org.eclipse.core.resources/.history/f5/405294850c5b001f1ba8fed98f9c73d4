import java.math.BigDecimal;

public class Proprietario {
	private int id;
	private String nome;
	private String descricao;
	private BigDecimal preco;
	
	//associação unidirecional com a classe Carro
	private Carro carro;
	
	public Proprietario() { 	
	}
	
	public Proprietario(int id, String nome, String descricao, BigDecimal preco, Carro carro) {
		super();
		this.id = id;
		this.nome = nome;
		this.descricao = descricao;
		this.preco = preco;
		this.carro = carro;
	}
	
	public Carro getCategoria() {
		return carro;
	}

	public void setCategoria(Carro carro) {
		this.carro = carro;
	}	

	//...implementação dos demais getters e setters.
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	public BigDecimal getPreco() {
		return preco;
	}

	public void setPreco(BigDecimal preco) {
		this.preco = preco;
	}

	@Override
	public String toString() {
		return "Proprietario [id=" + id + ", nome=" + nome + ", descricao=" + descricao + ", preco=" + preco + ", carro="
				+ carro + "]";
	}
	
	
	
}
